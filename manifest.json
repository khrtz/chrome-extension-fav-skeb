{
    "manifest_version": 3,
    "name": "Skeb fav manager",
    "version": "1.0",
    "description": "SkebのURLを管理します",
    "permissions": [
        "activeTab",
        "notifications",
        "scripting",
        "storage",
        "tabs"
    ],
    "host_permissions": [
      "https://skeb.jp/*"
    ],
    "action": {
        "default_title": "Skeb Fav Manager",
        "default_popup": "popup.html"
    }
  }
  